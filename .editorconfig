root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[vcbuild.bat]
end_of_line = crlf

[{lib,src,test}/**.js]
indent_style = space
indent_size = 4
quote_type = 'single'

[{lib,src,test}/**.ts]
indent_style = space
indent_size = 4
quote_type = 'single'
